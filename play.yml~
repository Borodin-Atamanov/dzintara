- hosts: all
  gather_facts: true
  tasks:
    - name: Ensure API key is present in config file
      ansible.builtin.lineinfile:
        path: passwords.yml
        line: "API_KEY={{ api_key }}"
		include_vars: passwords_uncrypted.yml
		debug: msg="Hello, {{ secrets.Pushok64.root_password }}!"
        
