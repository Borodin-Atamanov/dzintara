[config]
bm_open_method=1
home_path=/home/i/downloads/

[volume]
mount_on_startup=1
mount_removable=1
autorun=1

[autorun]
x-content/image-dcf=*

[ui]
always_show_tabs=1
max_tab_chars=75
win_width=1865
win_height=2123
splitter_pos=261
media_in_new_tab=0
desktop_folder_new_win=0
change_tab_on_drop=1
close_on_unmount=1
focus_previous=1
side_pane_mode=places
view_mode=list
show_hidden=1
sort=mtime;ascending;
toolbar=newtab;navigation;home;
show_statusbar=1
pathbar_mode_buttons=1
